/*
 *NumberLong Type의 데이터를 String으로 반환한다.;
 */
var numberLongToString = function( numberLong ){
	var _r = numberLong.toString();
	if( _r.indexOf( '"' ) == -1 ) return _r.replace("NumberLong(","").replace(")","");
	else return _r.replace("NumberLong(\"","").replace("\")","");
};

/*
 *NumberLong Type의 데이터를 Double로 반환한다.;
 */
var numberLongToNumber = function( numberLong ){
	var _r = numberLong.toString();
	if( _r.indexOf( '"' ) == -1 ) return Number( _r.replace("NumberLong(","").replace(")","") );
	else return Number( _r.replace("NumberLong(\"","").replace("\")","") );
};

var FN0 = function( nm_col ){
	//----------------------------------------------------------------------------------------------------;


	var year = nm_col.substr(0,4);
	var _goods_list_col = _goods_list_dbs[ year ].getCollection( nm_col );
	var _t_col = _t_dbs[ year ].getCollection( nm_col );
	var _t_m_col = _t_m_dbs[ year ].getCollection( nm_col );

	//----------------------------------------------------------------------------------------------------;
	/*
	{
		"_id" : date + "." + id_c + "." + _id
		"date" : 20180402,
		"_S_P" : {
			"_id" : NumberLong(1586899),

			//TaxA;
			"taxAmount" : 0.0,
		},
		"id_sh" : -1,
		"P_MAX" : 89.0,
		"P_MIN" : 89.0,
		"P" : 89.0,
		"OP_MAX" : 125.0,
		"OP_MIN" : 125.0,
		"OP" : 125.0,
		"N_CMT" : 0.0,
