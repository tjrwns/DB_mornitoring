			cd : "PM249",
			id_c : 21.0,
			AP : 38.0,//P;
			ShopNM :점포명,shop name
			SA : dailySales Amount
		}
	}
	*/
	//----------------------------------------------------------------------------------------------------;

	//;
	_t_m_col.drop();

	var i=0, iLen=_t_col.count();
	var _tO;
	_t_col.find({}).forEach( function( doc ){

		print( nm_col + " / "+ iLen + " / " + i + " - " + doc._id )

		_q1.product_no = numberLongToNumber( doc._id );
		var gDoc = _goods_list_col.findOne( _q1 );
//		printjson( _q1 )
//		printjson( gDoc )
		_tO = {
			"id_b" : NumberInt( -1 ),//id$brand_basic;
			"cd" : "",//cd$product_master;
			"id_c" : NumberInt( 26 ),
			"AP" : 0,
			"ShopNM" : "",
			"SA" : 0,
		};
		doc.date = NumberInt( nm_col );
		doc.N_SC_1 = Math.floor(Math.random() * 10000) + 1;
		doc._S_P._id = doc._id
		doc._id = nm_col + "." + _tO.id_c + "." + numberLongToString( doc._S_P._id )

		if( !gDoc ) return;

		try
		{
			_tO.id_b = NumberInt( gDoc._id$brand_basic );
			_tO.cd = gDoc.cd$product_master;
			_tO.AP = ( doc.P_MAX + doc.P_MIN ) / 2;
			_tO.ShopNM = doc._S_P.shopName;
			_tO.SA = doc.N_SC_1 * doc.P

		}
		catch( er )
		{
			//er;
		}


		doc._SYS0030 = _tO;

		//프로퍼티명 변경;
		doc._S_P.TaxA = doc._S_P.taxAmount;


		//deltet property;
		delete doc.nm;
		delete doc.id_c;
		delete doc.id_b;
		delete doc.id_se;
		delete doc.images;
		delete doc.S_SC;
		delete doc._S_P.taxFeeTitle;
		delete doc._S_P.taxAmount;
		delete doc._S_P.onlineStatus;
		delete doc._S_P.goodsStoreStatus;
		delete doc._S_P.promotionList;
		delete doc._S_P.shopHref;
		delete doc._S_P.shopName;
		delete doc._S_P.shopType;

		_t_m_col.insertOne( doc );
		++i;
	});
};

